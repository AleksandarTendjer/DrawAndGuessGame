          <a class="item__name" id="item__name<%= rooms[i].name %>"  onclick="debugger;enterRoom();" >
            <%= rooms[i].name %>
          <% }%>
        </li>
      <% } %>
      </ul>
    </div>
  </div>
  <nav class="navbar navbar-dark bg-dark">
